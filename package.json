{
  "name": "uscreen-video-player",
  "version": "0.1.45",
  "description": "Video player as customizable web-component",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "scripts": {
    "manifest": "custom-elements-manifest analyze",
    "manifest:watch": "pnpm manifest --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "hygen component new",
    "clean": "rm -rf dist types",
    "build": "pnpm run clean && pnpm run build:types && pnpm run build:lib",
    "build:lib": "vite build",
    "build:types": "tsc --declaration --emitDeclarationOnly"
  },
  "keywords": [
    "lit",
    "video-player",
    "hls",
    "web-component",
    "html5 video",
    "player"
  ],
  "author": "Uscreen <support@uscreen.tv> (https://www.uscreen.tv)",
  "license": "MIT",
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.8",
    "@open-wc/lit-helpers": "0.5.1",
    "@storybook/addon-essentials": "^7.0.0-beta.49",
    "@storybook/addon-links": "^7.0.0-beta.49",
    "@storybook/blocks": "^7.0.0-beta.49",
    "@storybook/web-components": "^7.0.0-beta.49",
    "@storybook/web-components-vite": "^7.0.0-beta.49",
    "@types/chromecast-caf-sender": "^1.0.5",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.13.0",
    "cem-plugin-better-lit-types": "0.2.0",
    "hygen": "^6.2.11",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.1",
    "postcss-preset-env": "^8.0.1",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.49",
    "typescript-json-schema": "^0.55.0",
    "vite": "^4.3.9",
    "web-component-analyzer": "2.0.0-next.4"
  },
  "optionalDependencies": {
    "hls.js": "^1.3.3",
    "lit": "^2.7.5",
    "lit-html": "^2.7.4",
    "mux-embed": "^4.26.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@lit-labs/context": "^0.2.0",
    "@lit/reactive-element": "^1.6.1",
    "@popperjs/core": "^2.11.6",
    "debug": "^4.3.4"
  }
}
