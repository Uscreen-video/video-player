{
  "font": "Roboto, sans-serif",
  "color-accent": "#006AFF",
  "color-danger": "#ED0B0B",

  "tooltip-background": "#232323",
  "tooltip-text": "#ffffff",
  "tooltip-font-size": "0.75rem",

  "button-color": "#ffffff",
  "button-size": "1.65rem",

  "menu-item-width": "215px",
  "menu-item-padding": ".5rem 0.75rem",
  "menu-item-background": "transparent",
  "menu-item-color": "white",
  "menu-item-background-active": "#232323",
  "menu-item-color-active": "white",
  "menu-item-background-hover": "#006AFF",
  "menu-item-color-hover": "white",

  "timer-color": "#ffffff",
  "timer-font-size": "0.75rem",

  "live-sign-color": "red",
  "live-sign-font-size": "0.75rem",
  "live-sign-text-color": "#ffffff",
  "live-sign-size": "0.5rem",

  "cue-font-size": "calc(14px + (26 - 14) * ((100vw - 300px) / (1600 - 300)))"
